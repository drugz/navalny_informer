{
    "manifest_version": 3,
    "name": "Привет, это Навальный!",
    "description": "Новостной информер самого важного сайта рунета. Россия Будущего будет счастливой",
    "version": "1.0.2",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/",
        "https://*/"
      ],
    "permissions": [       
        "storage",
        "alarms"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}