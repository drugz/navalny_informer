{
    "manifest_version": 3,
    "name": "HI! Navalny informer",
    "description": "Привет, это Навальный! Новостной информер самого важного сайта рунета. Россия Будущего будет счастливой",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/",
        "https://*/"
      ],
    "permissions": [       
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}